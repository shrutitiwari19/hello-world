#include<iostream.h>
#include<conio.h>
void main()
{
int age;
char name[50];
clrscr();

 Cout<<"\n Type the Name of the candidate: ";
 gets(name);

 Cout<<"\n Enter The Age : ";
 Cin>>age;

 if(age>=18)
  {
    Cout<<"\n %s is Eligible for Vote",<<name;
  }
 else
  {
    Cout<<"\n %s is Not Eligible for Vote",<<name;
  }

 getch();
}
